<ol>
    <div>

        <div class="container">
            <% _.each(items, function(topC) { %>
            
                <% if (topC.image) { %>
                <div>
                    <img  src="/images/<%= topC.image %>" data-src="holder.js/100px225?theme=thumb&amp;bg=55595c&amp;fg=eceeef&amp;text=Thumbnail" style="max-width: 100%; height: auto;">
                </div>
                <% } %>
                <div class="topstrip" style="width:100%;padding-bottom:20px; padding-top:20px;">
                     <% _.each(topC.categories, function(subC) { %>
                            <h2 class="container"><%= subC.name %></h2>
                            <div class="topstrip" style="width:100%;padding-bottom:20px; padding-top:20px;" >
